{
  "name": "opinion_bucket",
  "version": "1.0.0",
  "description": "A RESTful API and App code for Opinion Bucket",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "clean": "./setdb.sh",
    "test": "mocha --compilers js:babel-core/register test/server",
    "seed": "babel-node ./seed/seed.js --presets es2015,stage-2",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage": "rm -rf /coverage && node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha test/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bolorundurowb/Document-Management-System.git"
  },
  "keywords": [
    "Opinion",
    "Bucket"
  ],
  "author": "Bolorunduro Winner-Timothy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bolorundurowb/opinion-bucket/issues"
  },
  "homepage": "https://github.com/bolorundurowb/opinion-bucket#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "should": "^11.1.0",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "async": "^2.1.2",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "morgan": "^1.7.0"
  }
}
